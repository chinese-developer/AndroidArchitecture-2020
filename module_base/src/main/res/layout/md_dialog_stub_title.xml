<?xml version="1.0" encoding="utf-8"?>
<com.app.base.widget.dialog.mdstyle.core.main.DialogTitleLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/md_title_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >

  <ImageView
      android:id="@+id/md_icon_title"
      style="@style/MD_Dialog_Icon"
      tools:ignore="ContentDescription"
      />

  <com.app.base.widget.dialog.mdstyle.core.rtl.RtlTextView
      android:id="@+id/md_text_title"
      style="@style/MD_Dialog_Title_Text"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:visibility="gone"
      tools:text="RTL TextView .."
      tools:visibility="visible"
      />

</com.app.base.widget.dialog.mdstyle.core.main.DialogTitleLayout>
